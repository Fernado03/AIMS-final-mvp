# Configuration for AIMS Medical Scribe Application
# Copy this file to .env and replace placeholder values with your actual configuration

# Google Cloud Settings
GOOGLE_APPLICATION_CREDENTIALS=YOUR_SERVICE_ACCOUNT_JSON_PATH
GCS_BUCKET_NAME=YOUR_GCS_BUCKET_NAME

# Vertex AI Settings
VERTEX_AI_PROJECT_ID=YOUR_VERTEX_AI_PROJECT_ID 
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL_NAME=gemini-2.5-flash-preview-05-20

# Speech-to-Text Settings
SPEECH_LANGUAGE_CODE=en-US
SPEECH_MODEL=medical_conversation
SPEECH_ENABLE_AUTOMATIC_PUNCTUATION=true  # boolean (true/false, 1/0, yes/no)

# Database Settings
DATABASE_NAME=YOUR_DATABASE_NAME.db

# Add any additional environment variables below